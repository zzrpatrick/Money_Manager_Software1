CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE USERS(NAME VARCHAR(20) NOT NULL PRIMARY KEY, PAY INTEGER, CURRMONEY INTEGER)
CREATE MEMORY TABLE BILLS(OWNERNAME VARCHAR(20) NOT NULL, NAME VARCHAR(20), COST INTEGER, CONSTRAINT PK_BILL PRIMARY KEY (OWNERNAME, NAME), CONSTRAINT FK_BILL FOREIGN KEY(OWNERNAME) REFERENCES USERS(NAME))
CREATE MEMORY TABLE TRANSACTIONS(OWNERNAME VARCHAR(20) NOT NULL, NAME VARCHAR(20), COST INTEGER, CONSTRAINT PK_TRANSACTION PRIMARY KEY (OWNERNAME, NAME), CONSTRAINT FK_TRANSACTION FOREIGN KEY(OWNERNAME) REFERENCES USERS(NAME))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
